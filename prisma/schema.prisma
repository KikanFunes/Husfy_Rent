generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model ContratoArriendo {
  id            Int      @id @default(autoincrement())
  arrendatario  String
  correo        String
  valorBase     Float
  fechaInicio   DateTime
  fechaReajuste DateTime
  creadoEn      DateTime @default(now())
}
